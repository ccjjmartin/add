{
  "name": "aquifer",
  "paths": {
    "make": "drupal.make.yml",
    "lock": false,
    "settings": "settings",
    "build": "../www/drupal",
    "root": "root",
    "drush": "drush",
    "themes": {
      "root": "themes",
      "contrib": "themes/contrib",
      "custom": "themes/custom"
    },
    "modules": {
      "root": "modules",
      "contrib": "modules/contrib",
      "custom": "modules/custom",
      "features": "modules/features"
    },
    "files": {
      "root": "files"
    }
  },
  "extensions": {},
  "refreshCommands": [
    [
      "updb"
    ],
    [
      "cc",
      "all"
    ],
    [
      "en",
      "master"
    ],
    [
      "master-execute",
      "--scope=local"
    ],
    [
      "fra"
    ],
    [
      "rr"
    ]
  ]
}
